<h1>Timeline</h1>


<ul class="posts">
  {{#each posts}}
  <li>{{this.message}}</li> 
  <li>{{this.likes}} likes</li>
  <li>{{this.user_id.first_name}}</li>
  <li>{{dateFormat this.createdAt "dddd, MMMM Do YYYY, h:mm:ss a"}}</li>
  <br>
  {{/each}}
</ul>

<a class="new-post-link" href="/posts/new">New post</a>
